SELECT ce.numero_unico AS CODIGO, p.nome AS PESSOAS, c.nome AS CURSOS 
  FROM certificado AS ce
    INNER JOIN participante AS p ON p.id_participante = ce.id_participante
    INNER JOIN curso AS c ON c.id_curso = ce.id_curso;


select *
  from certificado c
  INNER JOIN participante p
    ON c.id_participante = p.id_participante


select c.numero_unico, c.data_hora_emissao
    , cur.nome curso, p.nome participante
    , a.nome assinador, ca.nome cargo
    from certificado c
    INNER JOIN curso cur 
      ON c.id_curso = cur.id_curso
    inner join participante p 
      ON c.id_participante = p.id_participante
    inner join assinante a
      on c.id_assinante = a.id_assinante
    inner join cargo ca
      on a.id_cargo = ca.id_cargo
    ;
